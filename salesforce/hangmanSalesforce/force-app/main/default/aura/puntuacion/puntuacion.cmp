<aura:component>
    <aura:attribute name="scores" type="Object[]"></aura:attribute>
    <table>
        <thead>
            <tr>
                <th>Fecha</th>
                <th>Puntuaci√≥n</th>
            </tr>
        </thead>
        <aura:iteration items="{!v.scores}" var="row" indexVar="index">
           <tr>
               <td>                   
                {!row.date}
               </td>
               <td>
                {!row.tries}
               </td>
           </tr> 
            </aura:iteration>
        </table>
        <aura:method name="insertScore" action="{!c.registerScore}">
            <aura:attribute name="tries" type="Integer"></aura:attribute>
        </aura:method>
</aura:component>	
